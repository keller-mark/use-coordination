    { "path": "packages/trrack-helpers" },
    { "path": "packages/graphviz-renderer" },
    { "path": "packages/flow-editor" },
    { "path": "packages/core" },
    { "path": "packages/main/all" },
    { "path": "packages/main/dev" },
    { "path": "packages/main/prod" },
    { "path": "examples/basic" },
    { "path": "examples/plots" },
    { "path": "examples/volumetric" }
  ],
  "files": []
