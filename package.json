{
  "name": "@use-coordination/root",
  "private": true,
  "version": "0.0.9",
  "description": "",
  "author": "Mark Keller",
  "homepage": "https://keller-mark.github.io/use-coordination/",
  "repository": "https://github.com/keller-mark/use-coordination/",
  "scripts": {
    "start": "pnpm -r --parallel start",
    "start-demo": "pnpm -r --parallel --filter=!docs start",
    "start-docs": "pnpm -r --parallel --filter=!demo start",
    "clean-start": "pnpm install && pnpm run clean && pnpm run build && pnpm run start-demo",
    "build:ci": "pnpm -r --workspace-concurrency 1 build",
    "build": "pnpm -r build",
    "build-json-schema": "pnpm -C packages/json-schema build-json-schema",
    "start-tsc": "tsc -b --watch",
    "build-tsc": "tsc --build",
    "bundle:ci": "pnpm -r --workspace-concurrency 1 bundle",
    "bundle": "pnpm -r bundle",
    "test": "pnpm -r --workspace-concurrency 1 test",
    "lint": "eslint \"packages/**/src/**/*.{js,jsx,ts,tsx}\" \"sites/*/src/**/*.{js,jsx,ts,tsx}\" \"examples/*/src/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "eslint --fix \"packages/**/src/**/*.{js,jsx,ts,tsx}\" \"sites/*/src/**/*.{js,jsx,ts,tsx}\" \"examples/*/src/**/*.{js,jsx,ts,tsx}\"",
    "clean": "pnpm -r exec -- rm -rf dist dist-tsc tsconfig.tsbuildinfo",
    "clean-deps": "pnpm -r exec -- rm -rf node_modules",
    "clean-all": "pnpm run clean && pnpm run clean-deps",
    "meta-update": "cross-env META_UPDATER_MODE=write pnpm meta-updater",
    "meta-dryrun": "cross-env META_UPDATER_MODE=dryrun pnpm meta-updater",
    "meta-version-only": "cross-env META_UPDATER_MODE=versiononly pnpm meta-updater",
    "publint": "pnpm -r --filter \"./packages/**\" exec publint",
    "changeset-status": "changeset status --since=origin/main",
    "changeset-version": "node .changeset/pre-changelog.mjs && changeset version && node .changeset/post-changelog.mjs && pnpm install",
    "changeset-publish": "pnpm publish --filter='./packages/**' --no-git-checks --access public && git tag v$(cat ./package.json | jq -r .version) && git push --follow-tags && node .changeset/get-release-notes.mjs && echo \"should-release=true\" >> $GITHUB_OUTPUT",
    "changeset-publish-dummy": "git tag v$(cat ./package.json | jq -r .version) && git push --follow-tags && node .changeset/get-release-notes.mjs && echo \"should-release=true\" >> $GITHUB_OUTPUT"
  },
  "devDependencies": {
    "@pnpm/meta-updater": "^0.0.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.16.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "jest-canvas-mock": "^2.3.0",
    "jsdom": "^20.0.0",
    "jsdom-worker": "^0.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.55.0",
    "serve-static": "^1.15.0",
    "terser": "^5.15.0",
    "typescript": "^4.7.4",
    "vite": "^4.3.0",
    "vitest": "^0.23.4",
    "cross-env": "^7.0.0",
    "json-diff-ts": "^1.2.5",
    "lodash-es": "^4.17.21",
    "publint": "^0.1.11",
    "@changesets/cli": "^2.17.0",
    "@changesets/get-github-info": "^0.5.2",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "gray-matter": "^4.0.3",
    "mdast-util-to-string": "^1.0.6",
    "remark-parse": "^7.0.1",
    "remark-stringify": "^7.0.3",
    "unified": "^8.3.2",
    "@playwright/test": "^1.40.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    }
  },
  "keywords": [],
  "license": "MIT"
}